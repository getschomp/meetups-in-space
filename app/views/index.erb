<p></p>

<div class="container">
  <form action= "/" method="post">
    <h4>Sumbit new Meetups:</h4>
    <div>
      <label for="name">Name:</label>
      <input type="text" id="name" name= "name" placeholder="Astronut Ball"/>
    </div>
    <div>
      <label for="description">Description:</label>
      <input type="textarea" id="description" name= "description" placeholder="Dress up that space suit"/>
    </div>
    <div>
      <label for="location">Location:</label>
      <input type="text" id="location" name="location" placeholder= "Nebula 5" />
    </div>

    <input type = "submit"/>
  </form>

  <% @meetups.each do |meetup| %>
  <h1><a href="/meetups/<%=meetup.id%>"><%= meetup.name %></h1>
    <% end %>
</div Id="container">
